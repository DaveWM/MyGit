<Application
    x:Class="MyGit.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MyGit"
    xmlns:converters="using:MyGit.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <converters:UnreadStatusConverter x:Key="ReadStatusConverter"></converters:UnreadStatusConverter>
            <converters:DateFormatConverter x:Key="DateFormatConverter"></converters:DateFormatConverter>
            <converters:InvertBooleanConverter x:Key="InvertBooleanConverter"></converters:InvertBooleanConverter>
            <converters:StringToUriConverter x:Key="StringToUriConverter"></converters:StringToUriConverter>
            <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"></converters:BooleanToVisibilityConverter>
            <converters:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter"></converters:InverseBooleanToVisibilityConverter>
            <converters:ToStringConverter x:Key="ToStringConverter"></converters:ToStringConverter>
            <converters:NotificationReasonConverter x:Key="NotificationReasonConverter"></converters:NotificationReasonConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>
